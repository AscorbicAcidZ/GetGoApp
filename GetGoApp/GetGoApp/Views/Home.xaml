<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GetGoApp.Views.Home" NavigationPage.HasBackButton="False" NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            <WebView x:Name="webView" HeightRequest="700" />
            <StackLayout Orientation="Horizontal" Spacing="10" HorizontalOptions="CenterAndExpand" >
                <Button Text="Home" Clicked="HomeButton_Clicked" />
                <Button Text="Menu" Clicked="MenuButton_Clicked" />
            </StackLayout>
           

        </StackLayout>
    </ContentPage.Content>
</ContentPage>