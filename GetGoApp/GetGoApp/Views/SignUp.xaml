<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GetGoApp.Views.Registration">
    <ContentPage.Content>
        <Grid HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" Padding="20">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="100"></RowDefinition>
            </Grid.RowDefinitions>
            <WebView Grid.Row="0" x:Name="webView" HeightRequest="700" />


            <Button Grid.Row="1" Text="Next"  Clicked="SaveButton_Clicked"  TextColor="White" BackgroundColor="#1C375C" CornerRadius="10" />

        </Grid>

    </ContentPage.Content>
</ContentPage>