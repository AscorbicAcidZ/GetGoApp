<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GetGoApp.Views.Others.AboutUs">
    <NavigationPage.TitleView>
        <Label Text="About us" HorizontalOptions="StartAndExpand"
                   VerticalOptions="CenterAndExpand"
                   FontAttributes="Bold"
                   FontSize="16"
               TextColor="Black"/>
    </NavigationPage.TitleView>
    <ContentPage.Content>
        <Grid HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" Padding="3"  BackgroundColor="#bcf4e4">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"></RowDefinition>
             
            </Grid.RowDefinitions>
            <WebView  Grid.Row="0" x:Name="webView" HeightRequest="800" />
        </Grid>
    </ContentPage.Content>
</ContentPage>